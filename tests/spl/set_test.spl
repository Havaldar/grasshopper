struct Node {}

procedure set_test1 (X: set<Node>) {
  var Y: set<Node>;
  assume X == Y ++ X;
  assert Y <= X;
}

procedure set_test2 (X: set<Node>) {
  var Y: set<Node>;
  Y := X ++ set<Node>();
  assert Y == X;
}

procedure set_test3 (X: set<Node>, x: Node) {
  assume x in X;
  assert X != set<Node>();
  assert set(x) <= X;
}
