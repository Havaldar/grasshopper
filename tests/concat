assume x ~= null;
prev := null;
curr := x;
assume curr ~= null;
prev := curr;
curr := f curr;
assume curr ~= null;
prev := curr;
curr := f curr;
assume curr = null;
f prev := y;

cut:
curr := x;
assume curr ~= null & curr ~= y;
curr := f curr; 
assume curr ~= null & curr ~= y;
curr := f curr;
assume curr = null | curr = y;
assume curr ~= y;
