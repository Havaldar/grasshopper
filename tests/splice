assume ~ Reach_f x y y;
assume Reach_f x null null;
assume Reach_f y null null;
assume Reach_f x i i;
assume join_f x y = null;

assume i ~= null;

prev := null;
curr := y;

assume curr ~= null;
prev := curr;
curr := f curr;

cut: 

assume curr ~= x;
prev := curr;
curr := f curr;

assume curr = null;

f prev := f i;
f i := y;

//assume join_f i prev = null;
//assume join_f i y ~= null;
//assume ~Reach_f y prev prev;

assume ~ Reach_f x null null;
