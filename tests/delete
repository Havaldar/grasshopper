assume Reach_f root x null;
assume Reach_f root null null;
assume x ~= null;

prev := null;
curr := root;

assume curr ~= x;
prev := curr;
curr := f curr;

cut: 

assume curr ~= x;
prev := curr;
curr := f curr;

assume curr = x;

f prev := f x;

assume root ~= x;

//assume  Reach_f root x null;

assume ~ Reach_f root null null;

