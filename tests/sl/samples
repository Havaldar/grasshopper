//some sample formula from SmallfootRG (to test the parser)

rear |-> top_front * lseg(top_front, back)

if _N==old1 && _M==old2 then Result==1 * addr1|->contents:nw1 * addr2|->tl:_M
                        else Result==0 * addr1|->contents:_N * addr2|->tl:_M

c|->contents:_x * (if _x == NULL then emp else _x|->_)

acq0|->tl:1 * flag0|->tl:1 * flag1|->tl:_x * Crit0|->tl:0 * y|->_

acq0|->tl:1 * flag0|->tl:1 * flag1|->tl:_x * Crit0|->tl:1 * TID==myTID

flag0 |-> tl:0 * acq1|->tl:0 * acq0 |-> tl:0 * turn |->_ * Crit0|->tl:0 * flag1 |->_ * Crit1|->tl:0 * y |->_

flag0 |-> tl:0 * turn |->_ * Crit0|->tl:0 * flag1 |->_ * (Crit1|->tl:0 * y |->_ || Crit1 |->tl:1) * acq0 |-> tl:0 * acq1|->_

CANT_WRITE |-> val:0 * READING |-> val:_x * (_x==1 ||_x==2)

myTID!= TID * CANT_WRITE |-> val:0 * READING |->val:_x * (
     (_x==1 * SLOT1 |-> val:_y * (_y==1 * DATA11 |->_ || _y==2 * DATA12 |->_))
  || (_x==2 * SLOT2 |-> val:_y *  (_y==1 * DATA21 |->_ || _y==2 * DATA22 |->_))
)
