
insert(lst, elt)
requires lseg(lst, null) * elt |-> null
ensures lseg(returned, null)
{
    if (lst = null)
        return elt;
    else {
        curr := lst;
        while ( Random && curr.next ~= null ) curr ~= null * lseg(lst, curr) * lseg(curr, null) {
            curr := curr.next;
        }
        elt.next := curr.next;
        curr.next := elt;
        return lst;
    }
}
