assume Reach_n x null null;
curr := x;
prev := null;

assume curr ~= null;
succ := n curr;
n curr := prev;
prev := curr;
curr := succ;

cut:

assume curr ~= null;

succ := n curr;
n curr := prev;
prev := curr;
curr := succ;

assume curr ~= null;
succ := n curr;
n curr := prev;
prev := curr;
curr := succ;

assume curr = null;
x := prev;
assume ~(Reach_n x null null);



