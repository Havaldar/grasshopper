assume Reach_f root null null;
curr := root;
prev := null;

assume curr ~= null;
succ := f curr;
f curr := prev;
prev := curr;
curr := succ;

assume curr ~= null;
succ := f curr;
f curr := prev;
prev := curr;
curr := succ;

assume curr ~= null;
succ := f curr;
f curr := prev;
prev := curr;
curr := succ;

assume ~(Reach_f prev null null);

assume curr = null;
root := prev;
assume ~(Reach_f root null null);

cut: